<!DOCTYPE html>
<html>
  <head>
    <% include ./partials/_head %>
  </head>
  <body>
  <% include ./partials/_nav %>
    <main class="mdl-layout__content">
    <div class="page-content tripio-content">
        <h2>All Trips</h2>
        <% trips.forEach(function(trip, index) { %>
            <%= trip.name %><br>
            <%= trip.tagline %><br>
            <%= trip.location %><br>
            <%= trip.summary %><br>
            <%= trip.stops[0].time %><br>
            <%= trip.stops[0].stop %><br>
            <%= trip.tags %><br><br><br>
        <% }) %>
    </div>
    </main>
    </div>
  </body>
</html>
